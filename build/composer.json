{
  "authors": [
    {
      "name": "UNB Libraries libsupport@unb.ca",
      "role": "Creator"
    }
  ],
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ]
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "cweagans/composer-patches": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "drupal/core-composer-scaffold": true,
      "phpstan/extension-installer": true
    },
    "discard-changes": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "description": "Drupal 10 component of educationhistory.lib.unb.ca",
  "extra": {
    "drupal-scaffold": {
      "locations": {
        "web-root": "./"
      }
    },
    "enable-patching": true,
    "installer-paths": {
      "core": [
        "type:drupal-core"
      ],
      "libraries/{$name}": [
        "type:drupal-library"
      ],
      "modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "Commands/contrib/{$name}": [
        "type:drupal-drush"
      ],
      "modules/custom/{$name}": [
        "type:drupal-custom-module"
      ],
      "profiles/custom/{$name}": [
        "type:drupal-custom-profile"
      ],
      "themes/custom/{$name}": [
        "type:drupal-custom-theme"
      ]
    }
  },
  "license": "MIT",
  "minimum-stability": "dev",
  "name": "unb-libraries/educationhistory.lib.unb.ca",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ],
  "require": {
    "drupal/color": "^1",
    "drupal/bartik": "^1",
    "drupal/core-composer-scaffold": "^10",
    "drupal/core-recommended": "^10",
    "drupal/core": "^10",
    "drupal/google_analytics": "^4",
    "drupal/health_check": "^1",
    "drupal/quickedit": "^1",
    "drupal/redis": "^1",
    "drupal/simple_sitemap": "^4",
    "unb-libraries/composer-drupal-scaffold": "^10"
  },
  "require-dev": {
    "unb-libraries/drupal-development-libraries": "^10"
  },
  "type": "project"
}